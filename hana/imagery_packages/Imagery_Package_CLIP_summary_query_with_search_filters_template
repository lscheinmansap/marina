select distinct
	CV.IMAGERY_PACKAGE_ID, 
	CV.VIDEO_CLIP_GUID,
	CV.CLIP_PATH,
	CV.FIRST_FRAME_FILEPATH,
	CV.FRAME_COUNT,
	IFNULL(H.FRAME_HIT_COUNT, 0) as FRAME_HIT_COUNT
from SEQUENCE_CLIP_SUMMARY_VIEW CV
left join 
	(select 
		F.VIDEO_CLIP_GUID,
	 	count(1) as FRAME_HIT_COUNT
	 from VIDEO_CLIP_FRAME F
	 inner join VIDEO V on V.IMAGERY_PACKAGE_ID = F.IMAGERY_PACKAGE_ID
	 where <FILTER EXPRESSIONS HERE>
	 -- EXAMPLE:
	 --where V.FPS = 30
	 --and F.SENSOR_RELATIVE_PITCH_ANGLE between -40 and -30
	 group by F.VIDEO_CLIP_GUID) as H 
on H.VIDEO_CLIP_GUID = CV.VIDEO_CLIP_GUID
	<IMAGERY PACKAGE ID FILTER EXPRESSION HERE>
	--EXAMPLE:
	-- where CV.IMAGERY_PACKAGE_ID = 'D21C0500AC22B43C170050022EA2A31A'