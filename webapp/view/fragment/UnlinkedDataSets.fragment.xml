<core:FragmentDefinition 
    xmlns="sap.m"
    xmlns:c="sap.suite.ui.commons"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:core="sap.ui.core">
    <List 
        id="unlinkedList" 
        mode="SingleSelectLeft"
        items="/unlinked">
        <headerToolBar>
            <Toolbar>
                <content>
                    <SearchField liveChange="searchModelDataSets" />
                </content>
            </Toolbar>
        </headerToolBar>
        <items>
            <StandardListItem
                title="{NAME}"
                description="{DESCRIPTION}"
                info="{CLASSIFICATION}"
                infoState="Success"
                wrapping="true"
                customData:dataSetId="{DATA_SET_ID}"
                 />
        </items>
    </List>
</core:FragmentDefinition>