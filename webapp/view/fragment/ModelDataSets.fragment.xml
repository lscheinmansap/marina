<core:FragmentDefinition 
    xmlns="sap.m"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:core="sap.ui.core">
    <List
        id="modelDataSetsList"
        mode="MultiSelect"
        items="/modelDataSets">
        <headerToolbar>
            <Toolbar>
                <content>
                    <SearchField liveChange="searchModelDataSets" />
                </content>
            </Toolbar>
        </headerToolbar>
        <items>
            <StandardListItem
                title="{NAME}"
                description="{DESCRIPTION}"
                info="{CLASSIFICATION}"
                infoState="Success"
                wrapping="true"
                customData:dataSetId="{DATA_SET_ID}"
                 />
        </items>
    </List>
</core:FragmentDefinition>